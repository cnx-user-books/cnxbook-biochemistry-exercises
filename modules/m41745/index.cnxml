            <entry>Hypothalamic diabetes insipidus</entry>
          </row>
          <row>
            <entry>B.</entry>
            <entry>Nephrogenic diabetes insipidus</entry>
          </row>
          <row>
            <entry>C.</entry>
            <entry>Gastroenteritis</entry>
          </row>
          <row>
            <entry>D.</entry>
            <entry>Kidney stones</entry>
          </row>
          <row>
            <entry>E.</entry>
            <entry>Edema</entry>
