          <row>
            <entry>C.</entry>
            <entry>Active Transport</entry>
          </row>
          <row>
            <entry>D.</entry>
            <entry>Diffusion </entry>
          </row>
          <row>
            <entry>E.</entry>
            <entry>Dialysis</entry>
