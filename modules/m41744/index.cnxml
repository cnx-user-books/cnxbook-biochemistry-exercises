            <entry>A.</entry>
            <entry>Trypsinogen</entry>
          </row>
          <row>
            <entry>B.</entry>
            <entry>Proelastase</entry>
          </row>
          <row>
            <entry>C.</entry>
            <entry>Procarboxypeptidase</entry>
          </row>
          <row>
            <entry>D.</entry>
            <entry>Chymotrypsinogen</entry>
          </row>
          <row>
            <entry>E.</entry>
            <entry>Pepsinogen</entry>
