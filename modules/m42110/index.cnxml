            <entry>C.</entry>
            <entry>Cortisol</entry>
          </row>
          <row>
            <entry>D.</entry>
            <entry>Natriuretic peptides</entry>
          </row>
          <row>
            <entry>E.</entry>
            <entry>Parathyroid hormone</entry>
          </row>
