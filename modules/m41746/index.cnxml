          <row>
            <entry>B.</entry>
            <entry>Complex II: Succinate dehydrogenase</entry>
          </row>
          <row>
            <entry>C.</entry>
            <entry>Cytochrome c</entry>
          </row>
          <row>
            <entry>D.</entry>
            <entry>Complex III: cytochromes b-c1</entry>
          </row>
          <row>
            <entry>E.</entry>
            <entry>Complex IV: cytochrome c oxidase</entry>
          </row>
        </tbody>
